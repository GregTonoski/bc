#! /usr/bin/bc -q

l = line_length()

max = 128

for (i = 2; i < max; ++i)
{
	obase=i
	if (obase < 17)
	{
		1 * i^(l - 1)
		1 * i^l
		1 * i^(l + 1)
	}
	else if (obase >= 17 && obase <= 99)
	{
		1 * i^(((l - 1)/3)$)
		1 * i^((l/3)$)
		1 * i^(((l + 1)/3)$)
	}
}

obase=2^18
1 * i^(100)
1 * i^(101)
1 * i^(103)

halt
